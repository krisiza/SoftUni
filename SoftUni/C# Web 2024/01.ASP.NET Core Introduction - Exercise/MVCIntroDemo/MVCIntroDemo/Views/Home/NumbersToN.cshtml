@{
    ViewBag.Title = "Nums 1 ..." + ViewBag.Count;
}

<h3>@ViewBag.Title</h3>
<ul>
    @for (int i = 1; i <= ViewBag.Count; i++)
    {
        <li>@i</li>
    }
</ul>

<form asp-controller="Home" asp-action="NumbersToN" method="post">
    <input type="number" id="count" name="count"  min="1" required/>
    <button type="submit">Submit</button>
</form>
